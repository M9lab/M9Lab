<?xml version="1.0" encoding="UTF-8"?>
<reverb>
  <room_size>0.70</room_size>
  <reverberance>0.60</reverberance>
  <damping>0.30</damping>
  <tone_low>0.70</tone_low>
  <tone_high>0.20</tone_high>
  <wet_gain>-3.0</wet_gain>
  <dry_gain>0.0</dry_gain>
  <stereo_width>1.00</stereo_width>
  <pre_delay>0.02</pre_delay>
</reverb>

<equalization>
  <filtertype>CurveEQ</filtertype>
  <curve>
    <point f="0" g="-24"/>
    <point f="200" g="-6"/>
    <point f="4000" g="-3"/>
    <point f="5000" g="-6"/>
    <point f="8000" g="-12"/>
    <point f="16000" g="-24"/>
  </curve>
</equalization>

<echo>
  <delay>0.05</delay>
  <decay>0.3</decay>
</echo>

<normalize>
  <peak>-1.0</peak>
</normalize>
